                        </a>
                        <ul class="sub-menu">
                            <li class="has-sub">
                                <a href="javascript:;"><b class="caret pull-right"></b>Request Assets</a>
                                <ul class="sub-menu"></ul>
                            </li>
                        </ul>
                    </li>
                    @if (User.IsInRole("Manager"))
                    {
